{
    "step-types": {
        "chunk-script-module-step": [ 
            {
                "@type-id": "custom.prefixbox.prefixboxProductExportFull",
                "@supports-parallel-execution": true,
                "@supports-site-context": true,
                "@supports-organization-context": false,
                "description": "",
                "module": "int_prefixbox_ai_search/cartridge/scripts/prefixbox/jobs/prefixboxProductExportFull.js",
                "read-function": "read",
                "process-function": "process",
                "write-function": "write",
                "total-count-function": "",
                "before-step-function": "beforeStep",
                "before-chunk-function": "",
                "after-chunk-function": "",
                "after-step-function": "afterStep",
                "chunk-size": 1000,
                "transactional": false,
                "timeout-in-seconds": "14400",
                "parameters": {
                    "parameter": [
                        {
                            "@name": "srcFolder",
                            "@type": "string",
                            "@required": "true",
                            "@trim": "true",
                            "description": "Source folder path (in IMPEX) to export the file.",
                            "default-value": "/src/prefixbox/feeds/products/"
                        },
                        {
                            "@name": "deleteFile",
                            "description": "Delete file after sucessfull upload to prefixbox",
                            "@type": "boolean",
                            "@required": "false",
                            "@trim": "true",
                            "default-value": true
                        }
                    ]
                },
                "status-codes": {
                    "status": [
                        {
                            "@code": "ERROR",
                            "description": "Used when the step failed with an error."
                        },
                        {
                            "@code": "OK",
                            "description": "Used when the step finished successfully."
                        }
                    ]
                }
            }
        ]
    }
}